{
    "manifest_version": 2,
    "name": "OdooUtils",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://johnscomputer.odoo.com/*"
            ],
            "css": ["extension/css/src/main.css"],
            "js": ["extension/js/utils/loaded.js", "extension/js/utils/sleep.js", "extension/js/src/index.js", "extension/js/utils/printPayload.js"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png"
    }
}