{
    "manifest_version": 2,
    "name": "OdooUtils",
    "version": "1.0.3",
    "content_scripts": [
        {
            "matches": [
                "https://johnscomputer.odoo.com/*",
                "https://johnscomputer-stagingbase-3836980.dev.odoo.com/*"
            ],
            "css": [
                "extension/css/src/functions/printArticle/main.css",
                "extension/css/src/functions/alertOrder/main.css",
                "extension/css/src/functions/fritteVote/main.css"
            ],
            "js": [
                "extension/js/utils/loaded.js",
                "extension/js/libs/JSPrintManager.js",
                "extension/js/libs/firebase.min.js",
                "extension/js/libs/firestore.min.js",
                "extension/js/utils/printer.js",
                "extension/js/src/functions/printArticle/main.js",
                "extension/js/src/functions/alertOrder/main.js",
                "extension/js/src/functions/removeBanner/main.js",
                "extension/js/src/functions/setPosUser/main.js",
                "extension/js/src/functions/setOrderUser/main.js",
                "extension/js/src/functions/hideUnkownProducts/main.js",
                "extension/js/src/functions/fritteVote/main.js",
                "extension/js/src/index.js"
            ],
            "html": [
                "extension/html/fritte.html"
            ]
        }
    ],
    "permissions": [
        "unlimitedStorage",
        "tabs",
        "webRequest"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png"
    }
}